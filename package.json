{
  "name": "tractor-tracking-app",
  "version": "1.0.0",
  "description": "Simple app for tracking vehicles on a map",
  "main": "index.js",
  "repository": "https://github.com/dominik-domanski/tractor-tracking-app.git",
  "author": "Dominik Domanski",
  "license": "MIT",
  "scripts": {
    "serve": "ts-node-dev --respawn --ignore-watch node_modules src/cli/serve.ts",
    "client": "cd ./client && yarn watch",
    "dev": "concurrently \"yarn serve\" \"yarn client\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "@types/webpack-dev-server": "^3.11.1",
    "concurrently": "^5.3.0",
    "html-webpack-plugin": "^5.0.0-beta.6",
    "nodemon": "^2.0.7",
    "ts-node-dev": "^1.1.1"
  }
}
